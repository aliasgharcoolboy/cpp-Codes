#include <iostream>
using namespace std;

int n = 20;

class queue{
     int* array;
     int front;
     int back;
     public:
     queue() {
        array = new int[n];
        front = -1;
        back = -1;
     }

     void push(int x){
        if(back == n-1){
            cout<<"quee overflown"<<endl;
            return;
        }
        back++;
        array[back] = x;

        if(front == -1){
            front++;
        }
     }

     void pop() {
        if(front == -1 || front>back){
            cout<<"quee empty"<<endl;
            return;
        }
        front++;
     }

     int peek() {
        if(front == -1 || front>back){
            cout<<"no element in quee"<<endl;
            return -1;
        }
        return array[front];
     }

     bool empty() {
        if(front == -1 || front>back){
            return true;
        }
        return false;
     }
};

int main () {
    queue q;
    q.push(2);
    q.push(3);
    q.push(4);
        q.push(1);

    cout<<q.peek()<<endl;
    q.pop();

    cout<<q.peek()<<endl;
    q.pop();

    cout<<q.peek()<<endl;
    q.pop();

    cout<<q.peek()<<endl;
    q.pop();

    cout<<q.peek()<<endl;
}
