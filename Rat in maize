#include <iostream>
using namespace std;

bool issafe(int** array,int x,int y,int n){
    if(x<n && y<n && array[x][y]==1){
        return true;
    }
    return false;
}

bool ratinmaze(int** array,int x,int y,int n, int** solutionarray){
    if(x==n-1 && y==n-1){
        solutionarray[x][y] = 1;
        return true;
    }
    if(issafe(array,x,y,n)){
        solutionarray[x][y] = 1;
        return true;
        if(ratinmaze(array,x+1,y,n,solutionarray)){
            return true;
        if(ratinmaze(array,x,y+1,n,solutionarray)){
            return true;
        }
        solutionarray[x][y] = 0;
        return false;
    }
    }
    return false;
}

int main () {
    int n;
    cout<<"Please input the size of maze"<<endl;
    cin>>n;

    int** array = new int*[n];
    for(int i=0;i<n;i++){
        array[i] = new int [n];
        for(int j=0;j<n;j++){
            cin>>array[i][j];
        }
    }

    int** solutionarray = new int*[n];
    for(int i=0;i<n;i++){
        solutionarray[i] = new int [n];
        for(int j=0;j<n;j++){
            solutionarray[i][j] = 0;
        }
    }

    if(ratinmaze(array,0,0,n,solutionarray)){
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                cout<<solutionarray[i][j];
            }
        }
    }
}
