#include <iostream>
using namespace std;

bool isPrime(int n){
    for(int i=2;i<=n-1;i++){
        if(n%i==0){
            return false;
        }
    }
    return true;
}

void prime(int num){
    
    for(int i=1;i<=num;i++){
        if(isPrime(i)==true){
            cout<<i<<endl;
        }
        
    }
    
}

int main()
{
    int num;
    cout<<"Input the number to which prime numbers are to be checked"<<endl;
    cin>>num;
    
    prime(num);
    
    
    
    
    

    return 0;
}
