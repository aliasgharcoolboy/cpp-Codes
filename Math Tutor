#include <iostream>
#include <ctime>
using namespace std;

int main()
{
    srand(time(0));
    int num1 = 1+rand()%100000;
    cout<<num1<<endl;
    
    int num2 = 1+rand()%100000;
    cout<<num2<<endl;
    
    char key;
    cout<<"Hey!Kido if you have solved it in your mind then Press 'a' to view solution"<<endl;
    cin>>key;
    if(key == 'a'){
        cout<<" "<<num1<<endl;
        cout<<"+"<<num2<<endl;
        cout<<" "<<num1+num2<<endl;
    }

    return 0;
}
