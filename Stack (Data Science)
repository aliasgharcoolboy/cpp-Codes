#include <iostream>
using namespace std;

int n = 100;

class stack{
    public:
      int* array;
      int top;

      stack(){
        array = new int[n];
        top = -1;
      }


void push(int x){
    if(top==n-1){
        cout<<"Stack overflow"<<endl;
        return;
    }
     top++;
     array[top] = x;
}

void pop(){
    if(top==-1){
        cout<<"Stack is empty"<<endl;
        return;
    }
    top--;
}

int Top(){
   if(top==-1){
    cout<<"stack is empty"<<endl;
    return -1;
   }
   return array[top];
}

bool empty() {
    return top ==-1;
}
};


int main ()  {
    stack st;
    st.push(1);
    st.push(2);
    st.push(3);
    cout<<st.Top()<<endl;
    st.pop();
    cout<<st.Top()<<endl;
    }
